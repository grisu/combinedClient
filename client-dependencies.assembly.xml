<assembly>
	<id>client-dependencies</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<unpack>true</unpack>
			<useTransitiveDependencies>true</useTransitiveDependencies>
			<useTransitiveFiltering>false</useTransitiveFiltering>
			<useProjectArtifact>false</useProjectArtifact>
			<outputDirectory>/</outputDirectory>
			<excludes>
				<exclude>grisu.frontend:gricli</exclude>
				<exclude>grisu.frontend:grisu-client</exclude>
				<exclude>grisu:grisu-commons</exclude>
				<exclude>grisu:grid-jcommons</exclude>
				<exclude>grith:jgrith</exclude>
				<exclude>grith:grid-session</exclude>
				<exclude>grisu.frontend:grisu-template-client</exclude>
				<exclude>grisu.frontend:grisu-client-swing</exclude>
				<exclude>grisu.frontend:grython</exclude>
				<exclude>grisu.frontend:gee</exclude>
				<exclude>grisu.frontend:grisu-benchmark</exclude>
				<exclude>junit:junit</exclude>
				<exclude>bouncycastle:bcprov-jdk15</exclude>
				<exclude>org.bestgrid.virtscreen:grisu-virtscreen</exclude>
				<exclude>nz.org.nesi:nesi-setup</exclude>
				<exclude>nz.org.nesi:phyml-grid</exclude>
			</excludes>
			<unpackOptions>
				<excludes>
					<exclude>**/INDEX.LIST</exclude>
					<exclude>**/MANIFEST.MF</exclude>
					<exclude>**/LICENSE.txt</exclude>
					<exclude>**/NOTICE.txt</exclude>
					<exclude>**/logback.xml</exclude>
					<exclude>**/logback.groovy</exclude>
				</excludes>
			</unpackOptions>
		</dependencySet>
	</dependencySets>


</assembly>

